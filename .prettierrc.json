{
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "plugins": ["prettier-plugin-tailwindcss"],
    "semi": true,
    "printWidth": 180,

    "bracketSameLine": true,
    "endOfLine": "lf",
    "useTabs": false,
    "quoteProps": "as-needed",
    "htmlWhitespaceSensitivity": "css",
    "singleAttributePerLine": false,

    "jsxSingleQuote": true
}
